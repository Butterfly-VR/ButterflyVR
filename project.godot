config_version=5

[application]

config/name="ButterflyVR"
run/main_scene="res://scenes/startup/loading.tscn"
config/use_custom_user_dir=true
config/project_settings_override="user://override.cfg"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

NetworkManager="*res://scenes/autoload/network_manager.tscn"
AvatarPackLoader="*res://scenes/autoload/avatar_pack_loader.gd"
GlobalWorldAccess="*res://scenes/autoload/world_access.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="many worlds"

[filesystem]

import/blender/enabled=false

[global_group]

Players=""
LocalPlayer=""
RemoteAvatarManagers=""

